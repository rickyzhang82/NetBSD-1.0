options		SWAPPAGER,VNODEPAGER,DEVPAGER
options		FFS
options		INET
options		"COMPAT_43"
options		"TCP_COMPAT_42"
options		KTRACE
options		"COMPAT_09"
options		"COMDEF_SPEED=B19200"
options		SCSI

config		"netbsd"	root on sd1 swap on sd1

